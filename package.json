  "devDependencies": {
    "@rollup/plugin-typescript": "^8.2.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.56.1",
    "rollup-plugin-terser": "^7.0.2",
    "standard-version": "^9.3.2",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5"
  }
